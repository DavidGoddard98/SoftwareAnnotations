<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

    

	<extension
		 point="net.sourceforge.plantuml.eclipse.diagramTextProvider">
		 
		 <diagramTextProvider
            id="plantuml.osm.generation"
            providerClass="plantuml.osm.generation.StateDiagramTextProvider"
            label="Automatic diagram generation"
            priority="10"
            >
      	 </diagramTextProvider>
    </extension>

    <extension
         point="net.sourceforge.plantuml.text.textDiagramProvider">
	
	     <textDiagramProvider
            id="net.sourceforge.plantuml.text.statemachineDiagramProvider"
            label="State Machines diagrams in text editor"
            diagramPrefix="@start_state_machine"
            diagramSuffix="@end_state_machine"
            >
      	 </textDiagramProvider>
         <textDiagramProvider
            id="plantuml.osm.generation.autoGenerateOSMDiagramProvider"
            label="Auto-Generate State Machines diagrams in text editor lalalal"
            diagramPrefix="@start_OSM_generation"
            diagramSuffix="@end_OSM_generation"
            >
      	 </textDiagramProvider>
    </extension>



	<extension
         point="net.sourceforge.plantuml.eclipse.linkOpener">
         
         <linkOpener
            linkOpenerClass="plantuml.osm.generation.StateLinkOpener">
     	 </linkOpener>
     	 
    </extension>
    
    
    
    
    <extension   
	     point="org.eclipse.ui.editors.annotationTypes">
	     <type
	        markerType="FSM.MARKER"
	        name="FSM.MARKER">
	     </type>
         <type
	        markerType="FSM.State.Highlight"
	        name="FSM.State.Highlight">
	     </type>
	     <type
	        markerType="FSM.Transition.Highlight_1"
	        name="FSM.Transition.Highlight_1">
	 	 </type>
	  	<type
	        markerType="FSM.Transition.Highlight_2"
	        name="FSM.Transition.Highlight_2">
	  	</type>
	  	<type
	        markerType="FSM.Transition.Highlight_3"
	        name="FSM.Transition.Highlight_3">
	  	</type>
	  	<type
	        markerType="FSM.Transition.Highlight_4"
	        name="FSM.Transition.Highlight_4">
	  	</type>
	  	<type
	        markerType="FSM.Transition.Highlight_5"
	        name="FSM.Transition.Highlight_5">
	  	</type>
	  	<type
	        markerType="FSM.Transition.Highlight_6"
	        name="FSM.Transition.Highlight_6">
	 	</type>	  
    </extension>
   
	
    <extension
